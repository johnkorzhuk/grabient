{
  "name": "grabient-ops",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "dev": "npx convex dev --once && concurrently -r npm:dev:web npm:dev:convex",
    "dev:web": "vite dev",
    "dev:ts": "tsc -b -w",
    "dev:convex": "npx convex dev",
    "build": "vite build && tsc --noEmit",
    "start": "node .output/server/index.mjs",
    "format": "prettier --write .",
    "lint": "tsc && eslint .  --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "use:local": "cat .env.local-backend > .env.local.backend && cat .env.local | grep -v 'CONVEX_\\|VITE_CONVEX' > .env.local.keys && cat .env.local.backend .env.local.keys > .env.local && rm .env.local.backend .env.local.keys && echo 'Switched to local backend'",
    "use:cloud": "cat .env.cloud > .env.local.backend && cat .env.local | grep -v 'CONVEX_\\|VITE_CONVEX' > .env.local.keys && cat .env.local.backend .env.local.keys > .env.local && rm .env.local.backend .env.local.keys && echo 'Switched to cloud backend'"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^2.0.53",
    "@ai-sdk/google": "^2.0.44",
    "@ai-sdk/groq": "^2.0.32",
    "@ai-sdk/openai": "^2.0.76",
    "@anthropic-ai/sdk": "^0.71.1",
    "@cf-wasm/resvg": "^0.3.2",
    "@convex-dev/aggregate": "^0.2.0",
    "@convex-dev/migrations": "^0.3.1",
    "@convex-dev/r2": "^0.8.1",
    "@convex-dev/react-query": "^0.0.0-alpha.11",
    "@convex-dev/workflow": "^0.3.3",
    "@google/genai": "^1.31.0",
    "@google/generative-ai": "^0.24.1",
    "@mantine/hooks": "^8.3.7",
    "@openrouter/ai-sdk-provider": "^1.5.4",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@repo/data-ops": "workspace:^",
    "@resvg/resvg-wasm": "^2.6.2",
    "@tanstack/react-query": "^5.89.0",
    "@tanstack/react-router": "^1.132.2",
    "@tanstack/react-router-with-query": "^1.130.17",
    "@tanstack/react-start": "^1.132.2",
    "@tanstack/react-store": "^0.8.0",
    "ai": "^5.0.106",
    "cloudflare": "^5.2.0",
    "clsx": "^2.1.1",
    "convex": "^1.29.1",
    "convex-helpers": "^0.1.106",
    "groq-sdk": "^0.37.0",
    "lucide-react": "^0.476.0",
    "nanoid": "^5.1.6",
    "openai": "^6.10.0",
    "react": "^19.2.1",
    "react-dom": "^19.2.1",
    "tailwind-merge": "^3.3.1",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@convex-dev/eslint-plugin": "^1.0.0",
    "@tailwindcss/vite": "^4.1.13",
    "@tanstack/eslint-config": "^0.3.2",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.3",
    "concurrently": "~9.2.1",
    "prettier": "^3.6.2",
    "tailwindcss": "^4.1.13",
    "typescript": "^5.9.2",
    "vite": "^7.1.5",
    "vite-tsconfig-paths": "^5.1.4"
  }
}
