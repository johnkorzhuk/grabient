{
    "name": "grabient",
    "version": "2.0.0",
    "description": "",
    "scripts": {
        "setup": "pnpm i && pnpm run build:data-ops",
        "build:data-ops": "pnpm run --filter data-ops build",
        "test": "pnpm run --filter data-ops test",
        "test:watch": "pnpm run --filter data-ops test:watch",
        "test:coverage": "pnpm run --filter data-ops test:coverage",
        "dev:user-application": "pnpm run --filter user-application dev",
        "dev:grabient-ops": "pnpm run --filter grabient-ops dev",
        "dev:grabient-ops:local": "pnpm run --filter grabient-ops-local backend:start && pnpm run --filter grabient-ops use:local && pnpm run --filter grabient-ops dev",
        "local:start": "pnpm run --filter grabient-ops-local backend:start",
        "local:stop": "pnpm run --filter grabient-ops-local backend:stop",
        "local:logs": "pnpm run --filter grabient-ops-local backend:logs",
        "local:dashboard": "powershell.exe -Command 'Start-Process http://localhost:6791'",
        "deploy:staging": "pnpm run build:data-ops && pnpm run --filter user-application deploy",
        "deploy:production": "pnpm run build:data-ops && pnpm run --filter user-application deploy:production",
        "dev:data-service": "pnpm run --filter data-service dev",
        "deploy:data-service": "pnpm run build:data-ops && pnpm run --filter data-service deploy",
        "db:studio": "pnpm run --filter data-ops db:studio",
        "db:seed": "pnpm run --filter data-ops db:seed"
    },
    "keywords": [],
    "author": "John Korzhuk",
    "license": "FSL-1.1-ALv2",
    "packageManager": "pnpm@10.14.0",
    "devDependencies": {
        "@types/node": "^22.18.12",
        "typescript": "^5.9.3"
    }
}
